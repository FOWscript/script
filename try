slide1:CreateToggle("Auto snipe", function(value)
    getgenv().snipe = value
    while getgenv().snipe == true do
        if game.PlaceId == 975820487 then
			while task.wait(0.2) do
                local id = getgenv().id
                task.spawn(function()
                local success444, response444 = pcall(function()
				if game:GetService("MarketplaceService"):GetProductInfo(tonumber(id)).IsForSale then
				local args = {
					[1] = id
				}

				game:GetService("ReplicatedStorage"):WaitForChild("BuyShop"):FireServer(unpack(args))

							end
						end)
						print("Checked")
						if not success444 then
							warn("(Likely Ratelimit / 429) Error:", response444)
						end
						end)
				end
            else
                game:GetService("TeleportService"):Teleport(975820487, LocalPlayer)
        end
    end
end)


