local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "FOW | Mega catsle", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Tab 1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Button!",
	Callback = function()
      		-- Define a function to teleport the user to the next UGC Crate
local function teleportToNextCrate()
    local targetCrateName = "Skull"
    local crates = workspace:GetDescendants()
    local user = game.Players.LocalPlayer

    -- Find the nearest UGC Crate
    local nearestCrate, minDistance = nil, math.huge
    for _, crate in ipairs(crates) do
        if crate.Name == targetCrateName then
            local distance = (crate.Position - user.Character.HumanoidRootPart.Position).magnitude
            if distance < minDistance then
                nearestCrate = crate
                minDistance = distance
            end
        end
    end

    -- Teleport the user to the nearest crate
    if nearestCrate then
        user.Character.HumanoidRootPart.CFrame = CFrame.new(nearestCrate.Position)
    end
end

-- Create a loop to continuously find and teleport to UGC Crates
while true do
    if game.Players.LocalPlayer.Character.Humanoid.Health > 0 then 
        for i, skulls in pairs(crates) do 
            if skulls.Name == "ProximityPrompt" then
    teleportToNextCrate()
    wait(1)
      fireproximitypromt(skulls)
end
  	end    
})

