local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "FOW | Mega catsle", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Main = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Main:AddButton({
    Name = "load tora script",
    Callback = function()
	  local tp = game.Players.LocalPlayer.Character.HumanoidRootPart
for i, v in pairs(game.workspace::GetChildren()) do
if v.Name == "Meshes/axion_Text" and v.Parent then
tp.CFrame = v.CFrame wait()
end
end
    end    
})
