MainTab:AddToggle({
	Name = "Snipe Ugc",
	Callback = function(Value)
        getgenv().snipe = Value
        while getgenv().snipe == true and task.wait() do
        if game.PlaceId == 975820487 then
            local id = getgenv().id
            local speed = getgenv().time
            while task.wait(speed) do
            task.spawn(function()
            local success444, response444 = pcall(function()
            if game:GetService("MarketplaceService"):GetProductInfo(tonumber(id)).IsForSale then
            local args = {
                [1] = id
            }
            
            game:GetService("ReplicatedStorage"):WaitForChild("BuyShop"):FireServer(unpack(args))
            
                        end
                    end)
                    print("Checked")
                    if not success444 then
                        warn("(Likely Ratelimit / 429) Error:", response444)
                    end
                    end)
            end
        else
            game:GetService("TeleportService"):Teleport(975820487, LocalPlayer)
        end
    end
end
})


OpTab:AddButton({
	Name = "V1",
	Callback = function()
        spawn(function()
            for i, v in pairs(game:GetDescendants()) do
                if v:IsA("RemoteEvent") then
                    v:FireServer(limid)
                end
                if v:IsA("BindableEvent") then
                    v:Fire(limid)
                end
                if v:IsA("RemoteFunction") then
                    v:InvokeServer(limid)  
                end
            end
        end)
  	end    
})

